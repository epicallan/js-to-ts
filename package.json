{
  "name": "@epicallan/js-to-ts",
  "version": "1.1.4",
  "description": "simple graphql framework @devinit",
  "main": "dist/main.js",
  "publish": {
    "public": true
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "start": "ts-node src/main.ts --dir examples",
    "build": "tsc"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "js to ts",
    "javascript to typescript",
    "js-to-ts",
    "js-ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/epicallan/js-to-ts.git"
  },
  "dependencies": {
    "fs-extra": "5.0.0",
    "lebab": "^2.7.7",
    "ramda": "0.25.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.1",
    "@types/node": "9.4.7",
    "@types/ramda": "0.25.20",
    "@types/yargs": "^11.0.0",
    "git-pre-hooks": "1.2.0",
    "release-it": "7.2.1",
    "ts-node": "^5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "bin": {
    "js-to-ts": "index.js"
  },
  "git-pre-hooks": {
    "pre-push": [
      "npm run lint",
      "npm run build"
    ]
  }
}
